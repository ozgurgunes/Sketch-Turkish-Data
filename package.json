{
  "name": "turkish-data",
  "description": "Turkish data supplier plugin, provides names, locations, business titles and dates in various formats.",
  "author": "Ozgur Gunes",
  "authorEmail": "o.gunes@gmail.com",
  "homepage": "https://github.com/ozgurgunes/Sketch-Turkish-Data",
  "license": "MIT",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozgurgunes/Sketch-Turkish-Data.git"
  },
  "bugs": {
    "url": "https://github.com/ozgurgunes/Sketch-Turkish-Data/issues"
  },
  "engines": {
    "sketch": ">=52"
  },
  "skpm": {
    "name": "Turkish Data",
    "manifest": "src/manifest.json",
    "main": "turkish-data.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/babel-preset": "^0.2.2",
    "@skpm/builder": "^0.7.11",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sketch": "^0.2.4",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@ozgurgunes/sketch-plugin-analytics": "^0.5.3"
  }
}
